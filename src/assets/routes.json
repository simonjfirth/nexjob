{
    "routes": [
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "platformErrorOverrides": [
      {
        "errorType": "NotFound",
        "serve": "/custom-404.html"
      }
    ],
    "mimeTypes": {
        "custom": "text/html"
    }
  }